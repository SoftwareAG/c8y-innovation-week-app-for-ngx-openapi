{
  "$schema": "https://typedoc.org/schema.json",
  "name": "Cumulocity Web SDK",
  "out": "./docs",
  "exclude": ["**/*.spec.ts"],
  "theme": "default",
  "includeVersion": true,
  "hideGenerator": true,
  "excludeProtected": true,
  "excludePrivate": true,
  "excludeInternal": true,
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "suppressExcessPropertyErrors": true,
    "suppressImplicitAnyIndexErrors": true,
  }
}
